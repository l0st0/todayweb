{
  "name": "todayweb",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "tsc": "turbo tsc",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter='./packages/*' && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "prettier": "latest",
    "turbo": "latest",
    "@todayweb/prettier": "*"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">= 8.19.0",
    "yarn": ">= 1.22.0"
  },
  "packageManager": "npm@8.19.0"
}
